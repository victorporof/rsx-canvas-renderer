{
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "babel"
  ],
  "extends": [
    "airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "max-len": [
      "error",
      {
        "code": 140
      }
    ]
  }
}